	<FrameworkElement.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary
					Source="WindowStyle.xaml" />
			</ResourceDictionary.MergedDictionaries>
			<ControlTemplate
				TargetType="{x:Type ToolTip}"
				x:Key="Tooltip">
				<FrameworkTemplate.Resources>
					<ResourceDictionary>
						<Style
							TargetType="{x:Type TextBlock}"
							x:Key="{x:Type TextBlock}">
							<Setter
								Property="Foreground"
								Value="#FFF5F5DC" />
						</Style>
					</ResourceDictionary>
				</FrameworkTemplate.Resources>
				<Border
					BorderBrush="#FF000000"
					BorderThickness="1">
					<Grid
						HorizontalAlignment="Center"
						Margin="2">
						<Panel.Background>
							<SolidColorBrush
								Color="DarkSlateGray"
								Opacity="0.7" />
						</Panel.Background>
						<ContentPresenter />
					</Grid>
				</Border>
			</ControlTemplate>
			<BitmapImage
				UriSource="Resources/poweroff.png"
				x:Key="PoweroffIcon" />
		</ResourceDictionary>
	</FrameworkElement.Resources>
	<FrameworkElement.Style>
		<Style
			BasedOn="{StaticResource CustomWindowStyle}"
			TargetType="{x:Type Window}" />
	</FrameworkElement.Style>
	<Control.Background>
		<SolidColorBrush
			Color="DarkSlateGray"
			Opacity="0.2" />
	</Control.Background>
	<Grid>
		<Grid
			Background="#FF2F4F4F"
			ZIndex="99">
			<FrameworkElement.Style>
				<Style
					TargetType="{x:Type Grid}">
					<Setter
						Property="Visibility"
						Value="Hidden" />
					<Style.Triggers>
						<DataTrigger
							Binding="{Binding Proxy}"
							Value="{x:Null}">
							<Setter
								Property="Visibility"
								Value="Visible" />
						</DataTrigger>
						<DataTrigger
							Value="False"
							Binding="{Binding Proxy.IsAttached}">
							<Setter
								Property="Visibility"
								Value="Visible" />
						</DataTrigger>
					</Style.Triggers>
				</Style>
			</FrameworkElement.Style>
			<Label
				Name="Guide">
				<FrameworkElement.Style>
					<Style
						TargetType="{x:Type Label}">
						<Setter
							Property="Visibility"
							Value="Hidden" />
						<Style.Triggers>
							<DataTrigger
								Binding="{Binding Proxy}"
								Value="{x:Null}">
								<Setter
									Property="Visibility"
									Value="Visible" />
							</DataTrigger>
						</Style.Triggers>
					</Style>
				</FrameworkElement.Style>
				<Viewbox>
					<TextBlock
						Foreground="#FFFF0000">
						<Run
							FontWeight="Bold"
							Foreground="#FFF5F5DC">Login to the spacemap.</Run>
						<LineBreak />
						<Run
							FontStyle="Italic">Please make sure that:</Run>
						<LineBreak />* you don't switch windows<LineBreak />after starting the game client<LineBreak />* the target account is enabled<LineBreak /><LineBreak />Recommended browser : <Run
							FontWeight="Bold">Firefox</Run></TextBlock>
				</Viewbox>
			</Label>
			<Label
				Name="Progress">
				<FrameworkElement.Style>
					<Style
						TargetType="{x:Type Label}">
						<Setter
							Property="Visibility"
							Value="Hidden" />
						<Style.Triggers>
							<DataTrigger
								Value="Hidden"
								Binding="{Binding ElementName=Guide, Path=Visibility}">
								<Setter
									Property="Visibility"
									Value="Visible" />
							</DataTrigger>
						</Style.Triggers>
					</Style>
				</FrameworkElement.Style>
				<Viewbox>
					<TextBlock
						Text="{Binding Proxy.Status}" />
				</Viewbox>
			</Label>
		</Grid>
		<Grid
			DataContext="{Binding Proxy.Game.Map.SelectedShip}">
			<Grid.ColumnDefinitions>
				<ColumnDefinition
					Width="2*" />
			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition
					Height="60" />
				<RowDefinition
					Height="*" />
				<RowDefinition
					Height="30" />
			</Grid.RowDefinitions>
			<UniformGrid
				Grid.Row="0"
				Grid.Column="0"
				Columns="2">
				<Border
					BorderThickness="2"
					BorderBrush="#FF000000"
					Margin="2">
					<Grid>
						<ProgressBar
							Value="{Binding HpPercentage, Mode=OneWay}" />
						<TextBlock
							HorizontalAlignment="Center"
							VerticalAlignment="Center"
							Text="{Binding HpDisplay, Mode=OneWay}" />
					</Grid>
				</Border>
				<Border
					BorderThickness="2"
					BorderBrush="#FF000000"
					Margin="2">
					<Grid>
						<ProgressBar
							Foreground="#FF1E90FF"
							Value="{Binding ShieldPercentage, Mode=OneWay}" />
						<TextBlock
							HorizontalAlignment="Center"
							VerticalAlignment="Center"
							Text="{Binding ShieldDisplay, Mode=OneWay}" />
					</Grid>
				</Border>
			</UniformGrid>
			<UniformGrid
				Grid.Row="2"
				Grid.Column="0"
